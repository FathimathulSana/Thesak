            <!-- Header -->
	<header class="header-v4">
		<!-- Header desktop -->
		<div class="container-menu-desktop">
			<!-- Topbar -->
			
			<div class="top-bar">
				<div class="content-topbar flex-sb-m h-full container">
					<div class="left-top-bar">
						Free shipping 
					</div>

					<div class="right-top-bar flex-w h-full">
						{{#if userLoggedIn}}
                         <a href="/userLogout" class="flex-c-m trans-04 p-lr-25">
						Logout
						</a>
						<a href="/profile" class="flex-c-m trans-04 p-lr-25">
							<img src="/admin/img/icons8-male-user-24.png" alt="profile">
						</a>
						{{else}}
						 <a href="/Login" class="flex-c-m trans-04 p-lr-25">
						Login/Signup
						</a>
						
						{{/if}}

						
            
					
					</div>
				</div>
			</div>

			<div class="wrap-menu-desktop">
				<nav class="limiter-menu-desktop container">
					
					<!-- Logo desktop -->		
					<a href="#" class="logo">
						<img src="/images/text2.png" alt="IMG-LOGO">
					</a>

					<!-- Menu desktop -->
					<div class="menu-desktop">
						<ul class="main-menu">
								<li>
								<a href="/">Home</a>
							</li>
							<li class="active-menu">
								<a href="" class="text-dark">Categories</a>
								<ul class="sub-menu">
									{{#each categoryDetails}}
									<li><a href="#">{{this.category}}</a></li>
									{{/each}}
									
								</ul>
							</li>

							<li>
								<a href="#">About</a>
							</li>

							<li>
								<a href="#">Contact</a>
							</li>
						</ul>
					</div>	

					<!-- Icon header -->
					<div class="wrap-icon-header flex-w flex-r-m">
						<div class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 js-show-modal-search">
							<i class="zmdi zmdi-search"></i>
						</div>

						  {{#if userLoggedIn}}
                          <a href="/viewCart"> 
                            <div class="icon-header-item cl2 hov-cl1 trans-04 p-r-11 p-l-10 icon-header-noti js-show-cart" data-notify="2" >
                                   <i class="zmdi zmdi-shopping-cart"></i>
                            </div></a>
                     {{else}}
                     <a href="/Login"> 
                                   <div class="icon-header-item cl2 hov-cl1 trans-04 p-r-11 p-l-10 icon-header-noti js-show-cart" data-notify="2" >
                                         <i class="zmdi zmdi-shopping-cart"></i>
                                       </div></a>
                                       {{/if}}
   
                       {{#if userLoggedIn}}
						<a href="/wishList" class="dis-block icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 icon-header-noti" data-notify="0">
							<i class="zmdi zmdi-favorite-outline"></i>
						</a>
						{{else}}
						<a href="/Login" class="dis-block icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 icon-header-noti" data-notify="0">
							<i class="zmdi zmdi-favorite-outline"></i>
						</a>
						{{/if}}
					</div>
				</nav>
			</div>	
		</div>

		<!-- Header Mobile -->
		<div class="wrap-header-mobile">
			<!-- Logo moblie -->		
			<div class="logo-mobile">
				<a href="#"><img src="images/text2.png" alt="IMG-LOGO"></a>
			</div>

			<!-- Icon header -->
			<div class="wrap-icon-header flex-w flex-r-m m-r-15">
				<div class="icon-header-item cl2 hov-cl1 trans-04 p-r-11 js-show-modal-search">
					<i class="zmdi zmdi-search"></i>
				</div>
             {{#if userLoggedIn}}
             <a href="/viewCart"> 
          <div class="icon-header-item cl2 hov-cl1 trans-04 p-r-11 p-l-10 icon-header-noti js-show-cart" data-notify="2" >
            <i class="zmdi zmdi-shopping-cart"></i>
          </div></a>
          {{else}}
            <a href="/Login"> 
          <div class="icon-header-item cl2 hov-cl1 trans-04 p-r-11 p-l-10 icon-header-noti js-show-cart" data-notify="2">
            <i class="zmdi zmdi-shopping-cart"></i>
          </div></a>
          {{/if}}
                  {{#if userLoggedIn}}
				<a href="/wishList" class="dis-block icon-header-item cl2 hov-cl1 trans-04 p-r-11 p-l-10 icon-header-noti" data-notify="0">
					<i class="zmdi zmdi-favorite-outline"></i>
				</a>
				{{else}}
					<a href="/Login" class="dis-block icon-header-item cl2 hov-cl1 trans-04 p-r-11 p-l-10 icon-header-noti" data-notify="0">
					<i class="zmdi zmdi-favorite-outline"></i>
				</a>
				{{/if}}
			</div>

			<!-- Button show menu -->
			<div class="btn-show-menu-mobile hamburger hamburger--squeeze">
				<span class="hamburger-box">
					<span class="hamburger-inner"></span>
				</span>
			</div>
		</div>


		<!-- Menu Mobile -->
		<div class="menu-mobile">
			<ul class="topbar-mobile">
				<li>
					<div class="left-top-bar">
						Free shipping 
					</div>
				</li>

				<li>
					<div class="right-top-bar flex-w h-full">
						{{#if userLoggedIn}}
                         <a href="/userLogout" class="flex-c-m trans-04 p-lr-25">
						Logout
						</a>
						<a href="/profile" class="flex-c-m trans-04 p-lr-25">
							<img src="/admin/img/icons8-male-user-24.png" alt="profile">
						</a>
						{{else}}
						 <a href="/Login" class="flex-c-m trans-04 p-lr-25">
						Login/Signup
						</a>
						
						{{/if}}

						
            
					
					</div>
				</li>
			</ul>

			<ul class="main-menu-m">
				<li>
					<a href="#">Home</a>
				</li>
				<li>
					<a href="#" class="text-dark">Categories</a>
					<ul class="sub-menu-m">
						{{#each categoryDetails}}
									<li><a href="#">{{this.category}}</a></li>
						{{/each}}
					</ul>
					<span class="arrow-main-menu-m">
						<i class="fa fa-angle-right" aria-hidden="true"></i>
					</span>
				</li>


				<li>
					<a href="about.html">About</a>
				</li>

				<li>
					<a href="contact.html">Contact</a>
				</li>
			</ul>
		</div>

		<!-- Modal Search -->
		<div class="modal-search-header flex-c-m trans-04 js-hide-modal-search">
			<div class="container-search-header">
				<button class="flex-c-m btn-hide-modal-search trans-04 js-hide-modal-search">
					<img src="images/icons/icon-close2.png" alt="CLOSE">
				</button>

				<form class="wrap-search-header flex-w p-l-15">
					<button class="flex-c-m trans-04">
						<i class="zmdi zmdi-search"></i>
					</button>
					<input class="plh3" type="text" name="search" placeholder="Search...">
				</form>
			</div>
		</div>
	</header>

	